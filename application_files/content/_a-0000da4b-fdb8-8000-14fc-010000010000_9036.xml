<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CMIS_returnNarrayElements</name>
        <uuid>_a-0000da4b-fdb8-8000-14fc-010000010000_9036</uuid>
        <description>Gets N elements of the array
For e.g. if you pass {a;b;c;d;e} and 3, this function will return {a;b;c}

Used for managing breadcrumbs</description>
        <parentUuid>_a-0000da4c-c8be-8000-14fc-010000010000_9157</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>IF (ri!index &gt; COUNT(ri!array), ri!array,
INDEX(ri!array, 1+enumerate(ri!index),ri!array)
)</definition>
        <namedTypedValue>
            <name>array</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
