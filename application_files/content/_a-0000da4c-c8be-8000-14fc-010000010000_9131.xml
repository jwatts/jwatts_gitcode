    <rule>
        <name>CMIS_getClickLinkForImage</name>
        <uuid>_a-0000da4c-c8be-8000-14fc-010000010000_9131</uuid>
        <description>For some reason, the getClickLink was not usable here. So switching to this one.
**TODO**
Refactor the code</description>
        <parentUuid>_a-0000da4c-c8be-8000-14fc-010000010000_9157</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if (ri!res[&quot;cmis:objectTypeId&quot;]=#&quot;_a-0000da4b-7cda-8000-14fc-010000010000_9010&quot;,
	#&quot;SYSTEM_SYSRULES_dynamicLink&quot;(
		value:ri!res[&quot;cmis:objectId&quot;],
		saveInto:{
					a!save(target:ri!selectedObject, value:save!value),
					a!save(target:ri!selectedObjectBreadcrumb, value:append(ri!selectedObjectBreadcrumb,{ri!res[&quot;cmis:objectId&quot;]})),			
					a!save(target:ri!selectedObjectBreadcrumbName, value:append(ri!selectedObjectBreadcrumbName,{ri!res[&quot;cmis:name&quot;]}))
					
				 },
		label:ri!res[&quot;cmis:name&quot;]
		),
		
		
		
#&quot;SYSTEM_SYSRULES_safeLink&quot;(
	  uri:#&quot;_a-0000da4c-0be2-8000-14fc-010000010000_9042&quot;(cmisObject:ri!res, cmisName:ri!cmisName),
	  label:ri!res[&quot;cmis:name&quot;]
	)

)
</definition>
        <namedTypedValue>
            <name>res</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>selectedObject</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>selectedObjectBreadcrumb</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>selectedObjectBreadcrumbName</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>cmisName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
