<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>validateNoOfCharBetween</name>
        <uuid>_a-0000da31-af1f-8000-14fc-010000010000_8924</uuid>
        <description>Validates if input is between the specified no of charactesr</description>
        <parentUuid>_a-0000da56-072d-8000-14fc-010000010000_9345</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=regexmatch(&quot;^[A-Za-z0-9_-]{&quot;&amp;ri!minChar&amp;&quot;,&quot;&amp;ri!maxChar&amp;&quot;}$&quot;, ri!input)</definition>
        <namedTypedValue>
            <name>input</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>minChar</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>maxChar</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
